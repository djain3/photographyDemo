          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".items-row, .gallery-item": {
              "@media (min-width: 992px)": {
                "&.lg-pb": {
                  "padding-bottom": "(@paddingCard * 2px)"
                }
              },
              "@media (max-width: 991px)": {
                "&.md-pb": {
                  "padding-bottom": "(@paddingCard * 2px)"
                }
              },
              "@media (max-width: 767px)": {
                "padding": "0",
                "&:not(.last-child)": {
                  "padding-bottom": "1rem"
                }
              }
            },
            ".gallery-img-wrap": {
              "&:after": {
                "background": "@imgOverlayColor"
              },
              "span": {
                "font-size": "~\"@{imgIconsSize}rem\"",
                "width": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
                "height": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
                "background-color": "@imgIconsBgColor",
                "color": "@imgIconsColor"
              }
            },
            "& when (@siteStyle = Photography)": {
              "& when (@flexDirection)": {
                ".mbr-px-4": {
                  "padding-left": "1rem",
                  "padding-right": "1rem",
                  "& when (@paddingCard < 16)": {
                    "padding-left": "0",
                    "padding-right": "0"
                  }
                },
                ".items-row, .gallery-item": {
                  "& when (@paddingCard < 16)": {
                    "@media (min-width: 992px)": {
                      "&.lg-pb": {
                        "padding": "0"
                      }
                    },
                    "@media (max-width: 991px)": {
                      "&.md-pb": {
                        "padding": "0"
                      }
                    }
                  }
                }
              }
            }
